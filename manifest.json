{
    "manifest_version": 3,
    "name": "QuickOpen",
    "description": "A quick and easy way to get up and running when first logging into your computer.",
    "version": "1.3",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "64": "images/icon-64.png",
      "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
      },
      "action": {
        "default_popup": "hello.html",
        "default_icon": {
          "16": "images/icon-16.png",
          "32": "images/icon-32.png",
          "64": "images/icon-64.png",
          "128": "images/icon-128.png"
        }
      }
  }